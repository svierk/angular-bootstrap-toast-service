{
  "name": "angular-bootstrap-toast-service",
  "version": "0.0.0",
  "author": {
    "name": "Sebastiano Vierk",
    "url": "https://github.com/svierk"
  },
  "license": "MIT",
  "engines": {
    "node": "14.18.1",
    "npm": "6.14.15"
  },
  "scripts": {
    "ng": "ng",
    "start": "node server.js",
    "start:dev": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "test:once": "ng test --watch=false",
    "test:coverage": "ng test --watch=false --code-coverage",
    "lint": "ng lint --max-warnings 0",
    "lint:fix": "ng lint --fix",
    "prettier": "prettier \"**/*.(ts|scss|html|js|json)\" --check",
    "prettier:fix": "prettier \"**/*.(ts|scss|html|js|json)\" --write",
    "heroku-postbuild": "ng build",
    "postinstall": "husky install",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "**/src/**": [
      "npm run prettier",
      "ng lint --max-warnings 0",
      "npm run test:once"
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~13.1.0",
    "@angular/cdk": "^13.1.1",
    "@angular/cli": "~13.1.2",
    "@angular/common": "~13.1.0",
    "@angular/compiler": "~13.1.0",
    "@angular/compiler-cli": "~13.1.0",
    "@angular/core": "~13.1.0",
    "@angular/forms": "~13.1.0",
    "@angular/material": "^13.1.1",
    "@angular/platform-browser": "~13.1.0",
    "@angular/platform-browser-dynamic": "~13.1.0",
    "@angular/router": "~13.1.0",
    "bootstrap": "^5.1.3",
    "express": "^4.17.2",
    "path": "^0.12.7",
    "rxjs": "~7.4.0",
    "tslib": "^2.3.0",
    "typescript": "~4.4.4",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.1.2",
    "@angular-eslint/builder": "13.0.1",
    "@angular-eslint/eslint-plugin": "13.0.1",
    "@angular-eslint/eslint-plugin-template": "13.0.1",
    "@angular-eslint/schematics": "13.0.1",
    "@angular-eslint/template-parser": "13.0.1",
    "@angular/cli": "~13.1.2",
    "@angular/compiler-cli": "~13.1.0",
    "@types/bootstrap": "^5.1.8",
    "@types/jasmine": "~3.10.0",
    "@types/node": "^12.11.1",
    "@typescript-eslint/eslint-plugin": "5.3.0",
    "@typescript-eslint/parser": "5.3.0",
    "eslint": "^8.2.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-deprecation": "^1.3.2",
    "eslint-plugin-jasmine": "^4.1.3",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-rxjs": "^4.0.4",
    "husky": "^7.0.4",
    "jasmine-core": "~3.10.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.1.0",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "karma-junit-reporter": "^2.0.1",
    "lint-staged": "^12.3.0",
    "prettier": "^2.5.1",
    "typescript": "~4.4.4"
  }
}
